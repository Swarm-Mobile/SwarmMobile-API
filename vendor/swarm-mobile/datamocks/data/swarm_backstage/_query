SELECT p.* 
FROM ping p
INNER JOIN device d 
ON p.device_id = d.id
AND (
	d.location_id IN (
		3,687,688,689,690,691,692, 
		693,1288,1340,1341,1342,50,
		51,63,64,70,71,230,231,232,
		233,262,266,330,331,911,1490,
		1491,1685,1686,768,769,1349,
		7,19,714,718,349,350,351,
		272,2039,1375
	) OR d.reseller_id IN(1,104,105)
);

