SELECT * 
FROM superadmin;

SELECT * 
FROM user
WHERE usertype_id IN (1,2)
UNION
SELECT u.*
FROM user u
INNER JOIN locationmanager lm
ON u.id = lm.user_id
INNER JOIN locationmanager_location lml
ON lml.locationmanager_id = lm.id
AND lml.location_id IN (
		3,687,688,689,690,691,692, 
		693,1288,1340,1341,1342,50,
		51,63,64,70,71,230,231,232,
		233,262,266,330,331,911,1490,
		1491,1685,1686,768,769,1349,
		7,19,714,718,349,350,351,
		272,2039,1375
)
UNION
SELECT u.*
FROM user u
INNER JOIN reseller r
ON u.id = r.user_id
INNER JOIN location l
ON l.reseller_id = r.id
AND r.id IN(1,104,105);

